<odoo>
  <template id="repair_order_report_template">
    <t t-call="web.html_container">
      <t t-foreach="docs" t-as="doc">
        <div class="page">
          <h2>Repair Order: <t t-esc="doc.name"/></h2>
          <p>Customer: <t t-esc="doc.partner_id.name"/></p>
          <p>Model: <t t-esc="doc.model_id.name"/></p>
          <p>Brand: <t t-esc="doc.brand_id.name"/></p>
          <p>Problem: <t t-esc="doc.problem"/></p>
        </div>
      </t>
    </t>
  </template>
</odoo>
